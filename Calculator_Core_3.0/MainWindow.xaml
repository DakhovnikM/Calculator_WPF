<Window x:Class="Calculator_Core_3._0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Calculator_Core_3._0"
        mc:Ignorable="d"
        Title="MainWindow" Height="420" Width="386">
    <Grid>
        <Button Name="ButtonChangeSign" Content="+/-" HorizontalAlignment="Left" Margin="10,323,0,0"
                VerticalAlignment="Top" Width="75" Height="40" Click="Button_ChangeSign_Click" FontSize="25" />
        <Button Name="Button0" Content="0" HorizontalAlignment="Left" Margin="100,323,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_0_Click" FontSize="25"/>
        <Button Name="Button8" Content="8" HorizontalAlignment="Left" Margin="100,188,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_8_Click" FontSize="25"/>
        <Button Name="Button5" Content="5" HorizontalAlignment="Left" Margin="100,233,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_5_Click" FontSize="25"/>
        <Button Name="Button2" Content="2" HorizontalAlignment="Left" Margin="100,278,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_2_Click" FontSize="25"/>
        <Button Name="ButtonReset" Content="С" HorizontalAlignment="Left" Margin="10,143,0,0" VerticalAlignment="Top" Width="165" Height="40" Click="Button_Reset_Click" FontSize="25"/>
        <Button Name="Button7" Content="7" HorizontalAlignment="Left" Margin="10,188,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_7_Click" FontSize="25"/>
        <Button Name="Button4" Content="4" HorizontalAlignment="Left" Margin="10,233,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_4_Click" FontSize="25"/>
        <Button Name="Button1" Content="1" HorizontalAlignment="Left" Margin="10,278,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_1_Click" FontSize="25"/>
        <Button Name="ButtonPoint" Content="," HorizontalAlignment="Left" Margin="192,323,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_Point_Click" FontSize="25"/>
        <Button Name="Button3" Content="3" HorizontalAlignment="Left" Margin="192,278,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_3_Click" FontSize="25"/>
        <Button Name="Button6" Content="6" HorizontalAlignment="Left" Margin="192,233,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_6_Click" FontSize="25"/>
        <Button Name="Button9" Content="9" HorizontalAlignment="Left" Margin="192,188,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_9_Click" FontSize="25"/>
        <Button Name="ButtonCorrect" Content="&lt;&lt;" HorizontalAlignment="Left" Margin="192,143,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="ButtonCorrect_Click" FontSize="25"/>
        <Button Name="ButtonDivision" Content="/" HorizontalAlignment="Left" Margin="282,143,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_Division_Click" FontSize="25"/>
        <Button Name="ButtonEquals" Content="=" HorizontalAlignment="Left" Margin="282,323,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_Equals_Click" FontSize="25"/>
        <Button Name="ButtonAddition" Content="+" HorizontalAlignment="Left" Margin="282,278,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_Addition_Click" FontSize="25"/>
        <Button Name="ButtonSubtraction" Content="-" HorizontalAlignment="Left" Margin="282,233,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_Subtraction_Click" FontSize="25"/>
        <Button Name="ButtonMultiplication" Content="*" HorizontalAlignment="Left" Margin="282,188,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Button_Multiplication_Click" FontSize="25"/>
        <TextBox Name="TextBox" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" HorizontalAlignment="Left" Height="116" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="347" FontSize="40"/>
        <Label Name="Label" HorizontalContentAlignment="Right" Content="Label" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="347" Height="34" FontSize="15" FlowDirection="LeftToRight"/>
    </Grid>
</Window>
